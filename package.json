{
  "name": "templatish",
  "version": "0.0.5",
  "description": "Template generator easy to customize",
  "main": "index.js",
  "scripts": {
    "start": "node bin/index.js",
    "pretty": "prettier --single-quote --no-semi --trailing-comma es5 --write \"./**/*.js\"",
    "patch-release": "npm version patch && npm publish && git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rowinbot/templatish.git"
  },
  "keywords": [
    "template",
    "generator",
    "javascript",
    "react",
    "vue"
  ],
  "author": "Rowin Hernandez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rowinbot/templatish/issues"
  },
  "homepage": "https://github.com/rowinbot/templatish#readme",
  "dependencies": {
    "inquirer": "^5.1.0",
    "prompt-confirm": "^1.2.0"
  },
  "bin": {
    "templatish": "index.js"
  },
  "devDependencies": {
    "prettier": "1.10.2"
  }
}
